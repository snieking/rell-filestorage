@external('filehub') @log
entity blockchain { 
	key rid: byte_array;
}

@external('filehub') @log
entity chunk {
	key hash: byte_array;
	pubkey;
	blockchain;
}

entity stored_file {
	key hash: byte_array;
	transaction;
}

struct gtx_operation {
    name;
    args: list<gtv>;
}
struct gtx_transaction {
     operations: list<gtx_operation>;
}